/**app.wxss**/

.clearfix::after {
  content: '';
  display: block;
  height: 0;
  visibility: hidden;
  clear: both;
}
.navigator-hover {
    background-color: rgba(0, 0, 0, 0); 
    opacity: 1;
}
text {
  font-size: 12px;
}

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 90;
  /*-webkit-backdrop-filter:blur(10px);*/
  background-color: rgba(0, 0, 0, 0.4);
  opacity: 0;
  transition: opacity 0.3s ease;
}

.mask.show {
  opacity: 1;
}

.right-arrow-2px {
  position: relative;
}

.right-arrow-2px::after {
  content: '';
  position: absolute;
  top: 5px;
  right: -12px;
  width: 8px;
  height: 8px;
  border-top: 2px solid #999;
  border-right: 2px solid #999;
  transform: rotate(45deg);
}

