page {
  height: 100%;
  background-color: #f8f8f8;
}

.address-list-wrapper .wx-radio-input-checked {
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
  border: 1rpx solid #dc0707;
}

.address-list-wrapper .wx-radio-input {
  width: 18px;
  height: 18px;
  border: 1rpx solid #ddd !important;
}

.address-list-wrapper .wx-radio-input.wx-radio-input-checked {
  border: 1rpx solid #dc0707 !important;
  background-color: #fff !important;
}

.address-list-wrapper .wx-radio-input.wx-radio-input-checked::before {
  content: '';
}

.address-list-wrapper {
  padding-bottom: 50px;
}

.address-list-wrapper .address-list {
}

.address-list-wrapper .address-list .list-item {
  background-color: #fff;
  margin-bottom: 10px;
}

.address-list-wrapper .address-list .list-item .content {
  border-bottom: 1rpx solid #ddd;
  padding: 12px;
}

.address-list-wrapper .address-list .list-item .top-area {
}

.address-list-wrapper .address-list .list-item .address-wrapper {
  line-height: 16px;
}

.address-list-wrapper .address-list .list-item .top-area .name {
  font-size: 14px;
  float: left;
}

.address-list-wrapper .address-list .list-item .top-area .phone {
  font-size: 14px;
  float: right;
}

.address-list-wrapper .address-list .list-item .content .address {
  line-height: 12px;
  font-size: 14px;
}

.address-list-wrapper .address-list .list-item .tool-bar {
  height: 24px;
  line-height: 24px;
  padding: 12px;
}

.address-list-wrapper .address-list .list-item .tool-bar .choose-radio {
}

.address-list-wrapper .address-list .list-item .tool-bar .choose-radio radio {
}

.address-list-wrapper .address-list .list-item .tool-bar .choose-radio .title {
}

.address-list-wrapper .address-list .list-item .tool-bar .btn {
  float: right;
  border: 1rpx solid #ddd;
  width: 50px;
  height: 24px;
  text-align: center;
  border-radius: 10px;
}

.address-list-wrapper .address-list .list-item .tool-bar .edit {
  margin-right: 6px;
}

.add-new {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 50px;
  line-height: 50px;
  background-color: #ff4500;
  text-align: center;
}
.add-new text {
  font-size: 14px;
  color: #fff;
}
