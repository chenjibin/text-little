.guess-like-wrapper {
  padding-bottom: 50px;
  padding-top: 50px;
  background-color: #f8f8f8;
}

.guess-like-wrapper .guess-like {
  width: 90%;
  margin: 0 auto;
  display: flex;
}

.guess-like-wrapper .guess-like .line {
  flex: 1;
  position: relative;
}

.guess-like-wrapper .guess-like .line::after {
  content: '';
  display: block;
  position: absolute;
  left: 0;
  top: 12px;
  width: 100%;
  border-top: 1rpx solid #ddd;
}

.guess-like-wrapper .guess-like .content {
  flex: 0 0 100px;
  width: 100px;
  height: 30px;
  text-align: center;
}

.guess-like-wrapper .guess-like .content .fs-icon {
  display: inline-block;
  vertical-align: middle;
  width: 24px;
  height: 24px;
  margin-right: 4px;
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  background-image: url("data:image/png;base64,");
}

.guess-like-wrapper .guess-like .content .title {
  display: inline-block;
  vertical-align: middle;
  color: #999;
  font-size: 15px;
}

.guess-like-wrapper .guess-goods {
  display: flex;
  flex-wrap: wrap;
  margin-top: 20px;
}

.guess-like-wrapper .guess-goods .each-good {
  flex: 0 0 50%;
  width: 50%;
  background-color: #fff;
  box-sizing: border-box;
  margin-bottom: 4px;
}
.guess-like-wrapper .guess-goods .each-good:nth-of-type(odd) {
  border-right: 2px solid #f8f8f8;

}
.guess-like-wrapper .guess-goods .each-good:nth-of-type(even) {
  border-left: 2px solid #f8f8f8;

}
.guess-like-wrapper .guess-goods .each-good .pic-wrapper {
}

.guess-like-wrapper .guess-goods .each-good .pic-wrapper image {
  max-width: 100%;
  height: 372rpx;
}

.guess-like-wrapper .guess-goods .each-good .content {
}

.guess-like-wrapper .guess-goods .each-good .content .title {
  display: -webkit-box;
  padding: 0 10rpx;
  line-height: 14px;
  height: 27px;
  -webkit-line-clamp: 2;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  overflow: hidden;
  font-size: 12px;
  color: #666;
  
}

.guess-like-wrapper .guess-goods .each-good .content .old-price {
  float: right;
  margin: 5px 6px 0 6px;
  font-family: 'Anton';
  color: #999;
