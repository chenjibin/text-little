  padding-left: 6px;
  padding-right: 12px;
}

.tooBar-wrapper .chooseAll-wrapper {
  display: flex;
  flex: 0 0 35px;
  width: 35px;
  align-items: center;
  justify-content: center;
}

.tooBar-wrapper .chooseAll-wrapper .choose-all {
}

.tooBar-wrapper .content {
  display: flex;
  flex: 1;
  align-items: center;
  justify-content: space-between;
}

.tooBar-wrapper .content .store-name {
  font-size: 14px;
}

.tooBar-wrapper .content .toolBtn-wrapper {
}

.tooBar-wrapper .content .toolBtn-wrapper .edit {
  display: inline-block;
  height: 36px;
  line-height: 36px;
  padding: 0 12px;
}

.payBar-wrapper {
  display: flex;
  position: fixed;
  padding-left: 6px;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 50px;
  line-height: 50px;
  border-bottom: 1rpx solid #ddd;
  border-top: 1rpx solid #ddd;
  box-sizing: border-box;
}

.payBar-wrapper .chooseAll-wrapper {
  flex: 0 0 60px;
  width: 60px;
}

.payBar-wrapper .content {
  flex: 1;
  
}

.payBar-wrapper .content text {
  float: right;
  margin-right: 6px;
}

.payBar-wrapper .content .heji {
  font-size: 14px;
}

.payBar-wrapper .content .money {
  font-size: 14px;
  color: #ff4500;
}

.payBar-wrapper .content .no-email {
  font-size: 10px;
  color: #999;
}

.payBar-wrapper .pay-btn {
  flex: 0 0 105px;
  width: 105px;
  text-align: center;
  background-color: #ff4500;
  color: #fff;
}

.payBar-wrapper .pay-btn text {
  font-size: 16px;
  font-weight: 700;
}
