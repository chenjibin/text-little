page {
  height: 100%;
  background-color: #f8f8f8;
}

.address-list-wrapper .wx-radio-input-checked {
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
  border: 1rpx solid #dc0707;
}

.address-list-wrapper .wx-radio-input {
  width: 18px;
  height: 18px;
  border: 1rpx solid #ddd !important;
}

.address-list-wrapper .wx-radio-input.wx-radio-input-checked {
  border: 1rpx solid #dc0707 !important;
  background-color: #fff !important;
}

.address-list-wrapper .wx-radio-input.wx-radio-input-checked::before {
  content: '';
}

.address-list-wrapper {
  position: absolute;
  left: 0;
  top: 0;
  bottom:50px;
  z-index: 90;
  width: 100%;
}

.address-list-wrapper .address-list {
}

.address-list-wrapper .address-list .list-item {
  background-color: #fff;
  margin-bottom: 10px;
}

.address-list-wrapper .address-list .list-item .content {
  border-bottom: 1rpx solid #ddd;
  padding: 12px;
}

.address-list-wrapper .address-list .list-item .top-area {
}

.address-list-wrapper .address-list .list-item .address-wrapper {
  line-height: 16px;
}

.address-list-wrapper .address-list .list-item .top-area .name {
  font-size: 14px;
  float: left;
}

.address-list-wrapper .address-list .list-item .top-area .phone {
  font-size: 14px;
  float: right;
}

.address-list-wrapper .address-list .list-item .content .address {
  line-height: 12px;
  font-size: 14px;
}

.address-list-wrapper .address-list .list-item .tool-bar {
  height: 24px;
  line-height: 24px;
  padding: 12px;
}

.address-list-wrapper .address-list .list-item .tool-bar .choose-radio {
  display: inline-block;
}

.address-list-wrapper .address-list .list-item .tool-bar .choose-radio radio {
  display: inline-block;
  vertical-align: middle;
  font-size: 0;
}

.address-list-wrapper .address-list .list-item .tool-bar .choose-radio .title {
  display: inline-block;
  vertical-align: middle;
}
.address-list-wrapper .address-list .list-item .tool-bar .choose-radio .title.default {
  color:#dc0707;
}
.address-list-wrapper .address-list .list-item .tool-bar .btn {
  float: right;
  border: 1rpx solid #ddd;
  width: 50px;
  height: 24px;
  text-align: center;
  border-radius: 10px;
}

.address-list-wrapper .address-list .list-item .tool-bar .edit {
  margin-right: 6px;
}

.add-new {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 50px;
  line-height: 50px;
  background-color: #ff4500;
  text-align: center;
}

.add-new text {
  font-size: 14px;
  color: #fff;
}

.save-btn,.cancel-btn {
  position: fixed;
  bottom: 0;
  left: 0;
  height: 50px;
  width: 50%;
  background-color: #fff;
  border-radius: 0 !important;
  box-sizing: border-box;
}
.save-btn {
  left: 50%;
  border-left: 1rpx solid #ddd;
}
.save-btn::after,.cancel-btn::after  {
  border: 0;
  border-top: 1rpx solid #999;
  border-radius: 0;
  box-sizing: border-box;
}

.form-wrapper {
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  width: 100%;
  z-index: 100;
  background-color: #fff;
  transition: transform 0.3s ease-out;
  transform: translate3d(0, -100%, 0);
}

.form-wrapper.show {
  transform: translate3d(0, 0, 0);
}

.form-wrapper .each-item {
  background-color: #fff;
  color: #666;
  border-bottom: 1rpx solid #ddd;
}

.form-wrapper .each-item.fs-input, .form-wrapper .each-item.fs-switch,
.form-wrapper .each-item.fs-picker {
  display: flex;
  height: 50px;
  line-height: 50px;
  padding: 0 12px;
}

.form-wrapper .each-item.fs-picker .area {
  font-size: 14px;
}

.form-wrapper .each-item.fs-picker .title {
  flex: 1;
}

.form-wrapper .each-item.fs-switch .title {
  flex: 1;
}

.form-wrapper .each-item.fs-switch switch {
  flex: 0 0 60px;
  width: 60px;
}

.form-wrapper .each-item.spec {
  margin-bottom: 10px;
}

.form-wrapper .each-item.spec textarea {
  width: 100%;
  padding: 16px 12px;
  box-sizing: border-box;
  font-size: 14px;
  height: 100px;
  color: #666;
}

.form-wrapper .each-item .title {
  flex: 0 0 65px;
  width: 65px;
  font-size: 14px;
}

.form-wrapper .each-item input {
  flex: 1;
  height: 100%;
}
