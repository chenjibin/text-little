.container {
  padding-bottom: 50px;
}

.good-swiper {
  text-align: center;
  height: 500rpx;
}

.good-swiper image {
  width: 80%;
  height: 500rpx;
}

.content-wrapper {
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  padding-bottom: 50px;
}

.content-wrapper .good-info {
  font-size: 0;
}

.content-wrapper .good-info>.title {
  font-size: 12px;
  display: block;
}

.content-wrapper .good-info>.spec {
  font-size: 12px;
  color: #dc0707;
}

.content-wrapper .good-info .info {
  margin-top: 10rpx;
  width: 100%;
  font-size: 14px;
  font-weight: 700;
  border-bottom: 1rpx solid #ddd;
}

.content-wrapper .good-info .num {
  font-family: 'Anton';
  font-weight: 500;
  margin-left: 2px;
}

.content-wrapper .good-info .info .new-price {
  display: inline-block;
  margin-right: 20rpx;
  vertical-align: middle;
}

.content-wrapper .good-info .info .new-price text {
  font-size: 18px;
}

.content-wrapper .good-info .info .old-price {
  display: inline-block;
  margin-right: 20rpx;
  vertical-align: middle;
  color: #999;
}

.content-wrapper .good-info .info .old-price text {
  font-size: 14px;
}

.content-wrapper .good-info .info .sale-num {
  display: inline-block;
  vertical-align: middle;
}

.content-wrapper .good-info .info .sale-num text {
  font-size: 14px;
}

.content-wrapper .good-info .server {
  font-size: 12px;
  height: 36px;
  line-height: 36px;
  border-bottom: 1rpx solid #ddd;
}

.content-wrapper .good-info .server .item-wrapper {
  display: inline-block;
  vertical-align: top;
  margin-right: 20rpx;
}

.content-wrapper .good-info .server .item-wrapper .fs-icon {
  display: inline-block;
  vertical-align: middle;
  margin-right: 2px;
  width: 16px;
  height: 16px;
  line-height: 16px;
  text-align: center;
  background-color: #333;
  border-radius: 2px;
  color: #fff;
}

.content-wrapper .good-info .server .item-wrapper .title {
  display: inline-block;
  vertical-align: middle;
}

.content-wrapper .good-info .server .item-wrapper .fs-icon.bao {
  background-color: #f7412d;
}

.content-wrapper .good-info .server .item-wrapper .fs-icon.huo {
  background-color: #48b04b;
}

.content-wrapper .good-info .server .item-wrapper .fs-icon.qi {
  background-color: #3e4eb8;
}

.content-wrapper .good-info .server .item-wrapper .fs-icon.shi {
  background-color: #dc0707;
}

.content-wrapper .good-info .btn-choose {
  height: 36px;
  line-height: 36px;
  font-size: 14px;
  color: #666;
  border-bottom: 1rpx solid #ddd;
}

.content-wrapper .good-info .btn-choose .item {
  font-size: 14px;
  margin-right: 6px;
  color: #333;
}

.content-wrapper .good-info .btn-choose .right-arrow {
  display: block;
  float: right;
  margin-top: 9px;
  margin-right: 10px;
  width: 14px;
  height: 14px;
  transform: rotate(45deg);
  border-top: 1px solid #ddd;
  border-right: 1px solid #ddd;
}

.bottom-bar-wrapper {
  display: flex;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 100;
  width: 100%;
  height: 50px;
}

.bottom-bar-wrapper .tab-group {
  flex: 0 0 150px;
  width: 150px;
}

.bottom-bar-wrapper .tab-group .tab-item {
  display: inline-block;
  width: 50px;
  height: 50px;
  text-align: center;
}

.bottom-bar-wrapper .tab-group .tab-item .fs-icon {
  display: block;
  margin: 6px auto 0 auto;
  width: 26px;
  height: 26px;
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
}

.bottom-bar-wrapper .tab-group .tab-item .fs-icon.home {
  background-image: url("data:image/png;base64,");
}

.bottom-bar-wrapper .tab-group .tab-item .fs-icon.cate {
  background-image: url("data:image/png;base64,");
}

.bottom-bar-wrapper .tab-group .tab-item .fs-icon.shoppingcart {
  background-image: url("data:image/png;base64,");
}

.bottom-bar-wrapper .tab-group .tab-item .title {
  display: block;
  font-size: 12px;
  color: #999;
}

.bottom-bar-wrapper .btn-group {
  flex: 1;
}

.bottom-bar-wrapper .btn-group button {
  display: inline-block;
  vertical-align: top;
  width: 50%;
  height: 50px;
  line-height: 50px;
  border: 0;
  background-color: #fff;
  font-size: 12px;
  border-radius: 0;
  color: #fff;
}

.bottom-bar-wrapper .btn-group button::after {
  border: 0;
  border-radius: 0;
}

.bottom-bar-wrapper .btn-group #addToCart {
  background-color: #ff4500;
}

.bottom-bar-wrapper .btn-group #buyNow {
  background-color: #dc0707;
}

.shoppingcart-wrapper {
  position: fixed;
  z-index: 200;
  width: 100%;
  bottom: 0;
  left: 0;
  background-color: #fff;
  box-sizing: border-box;
  transform: translate3d(0, 100%, 0);
  transition: transform 0.3s ease-out;
}

.shoppingcart-wrapper.show {
  transform: translate3d(0, 0, 0);
}

.shoppingcart-wrapper .head-wrapper {
  display: flex;
  padding: 0 12px;
}

.shoppingcart-wrapper .head-wrapper image {
  display: none;
  position: relative;
  top: -16px;
  flex: 0 0 90px;
  width: 90px;
  height: 90px;
  border: 1rpx solid #eee;
  box-sizing: border-box;
  border-radius: 6px;
}

.shoppingcart-wrapper.show .head-wrapper image {
  display: block;
}

.shoppingcart-wrapper .head-wrapper .content {
  flex: 1;
  padding: 10px 12px 0 12px;
  font-size: 0;
}

.shoppingcart-wrapper .head-wrapper .content .price text {
  font-size: 14px;
}

.shoppingcart-wrapper .head-wrapper .content .kucun text {
  font-size: 14px;
}

.shoppingcart-wrapper .head-wrapper .content .desc text {
  font-size: 14px;
}

.shoppingcart-wrapper .head-wrapper .content .desc .item {
  margin-right: 4px;
}

.shoppingcart-wrapper .sku-wrapper {
  padding: 0 12px;
}

.shoppingcart-wrapper .sku-wrapper .sku-item-wrapper {
  margin-bottom: 10px;
}

.shoppingcart-wrapper .sku-wrapper .sku-item-wrapper .title-wrapper {
}

.shoppingcart-wrapper .sku-wrapper .sku-item-wrapper .title-wrapper .title {
  font-size: 14px;
}

.shoppingcart-wrapper .sku-wrapper .sku-item-wrapper .option-group {
  font-size: 0;
  display: flex;
  flex-wrap: wrap;
  margin-top: 12px;
  border-bottom: 1rpx solid #ddd;
}

.shoppingcart-wrapper .sku-wrapper .sku-item-wrapper .option-group .option-item {
  display: inline-block;
  font-size: 12px;
  padding: 6px 8px;
  background-color: #eee;
  color: #666;
  border-radius: 6px;
  margin: 0 8px 8px 0;
}

.shoppingcart-wrapper .sku-wrapper .sku-item-wrapper .option-group .option-item.select {
  background-color: #dc0707;
  color: #fff;
}

.shoppingcart-wrapper .sku-wrapper .sku-item-wrapper .option-group .option-item.out-of-stock {
  background: #f8f8f8;
  color: #ddd;
}

.shoppingcart-wrapper .choosenum-wrapper {
  padding: 0 12px;
  margin-bottom: 10px;
}

.shoppingcart-wrapper .choosenum-wrapper .title {
  font-size: 14px;
}

.shoppingcart-wrapper .choosenum-wrapper .num-wrapper {
  float: right;
}



.shoppingcart-wrapper .choosenum-wrapper .num-wrapper .btn {
  display: inline-block;
  vertical-align: middle;
  margin-right: 4px;
  width: 30px;
  height: 30px;
  line-height: 30px;
  background-color: #eee;
  text-align: center;
  font-size: 20px;
  font-weight: 700;
  color: #999;
}



.shoppingcart-wrapper .choosenum-wrapper .num-wrapper label {
  display: inline-block;
  vertical-align: middle;
  margin-right: 4px;
}

.shoppingcart-wrapper .choosenum-wrapper .num-wrapper input {
  width: 40px;
  height: 30px;
  background-color: #eee;
  text-align: center;
  font-weight: 700;
  color: #666;
}

.shoppingcart-wrapper .btngroup-wrapper {
  display: flex;
  width: 100%;
  height: 50px;
}

.shoppingcart-wrapper .btngroup-wrapper .btn-one {
  flex: 1;
  color: #fff;
  text-align: center;
  line-height: 50px;
}

.shoppingcart-wrapper .btngroup-wrapper text {
  font-size: 14px;
}

.shoppingcart-wrapper .btngroup-wrapper .btn-two {
  flex: 1;
  color: #fff;
  text-align: center;
  line-height: 50px;
  background-color: #ff4500;
}

.shoppingcart-wrapper .btngroup-wrapper .add-cart {
  background-color: #ff4500;
}

.shoppingcart-wrapper .btngroup-wrapper .buy-now {
  background-color: #dc0707;
}
