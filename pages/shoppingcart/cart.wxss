.check-group {
}

.check-group .each-item {
  display: flex;
  margin-bottom: 6px;
  padding-left: 6px;
  background-color: #f6f6f6;
}

.check-group .each-item .check-wrapper {
  display: flex;
  align-items: center;
  justify-content: center;
  flex: 0 0 35px;
  width: 35px;
}

.check-group .each-item .check-wrapper checkbox {
}

.check-group .each-item .mian-content {
  display: flex;
}

.check-group .each-item .mian-content .good-pic {
  flex: 0 0 75px;
  width: 75px;
  height: 75px;
  margin: 10px 0;
}

.check-group .each-item .mian-content .content {
  display: flex;
  flex: 1;
  position: relative;
  overflow: hidden;
  padding: 10px 0;
}

.check-group .each-item .mian-content .content .toggle-box {
  display: flex;
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: #f6f6f6;
  transform: translate3d(0, -100%, 0);
  transition: transform 0.2s ease-out;
}

.check-group .each-item .mian-content .content .toggle-box.show {
  transform: translate3d(0, 0, 0);
}

.check-group .each-item .mian-content .content .toggle-box .input-wrapper {
  display: flex;
  flex: 1;
  padding: 10px 10px 0 10px;
}

.check-group .each-item .mian-content .content .toggle-box .input-wrapper .btn {
  flex: 0 0 30px;
  width: 30px;
  height: 30px;
  line-height: 30px;
  background-color: #eee;
  text-align: center;
  font-size: 20px;
  font-weight: 700;
  color: #999;
}

.check-group .each-item .mian-content .content .toggle-box .input-wrapper .input-box {
  flex: 1;
}

.check-group .each-item .mian-content .content .toggle-box .input-wrapper .input-box input {
  height: 30px;
  text-align: center;
  background-color: #fff;
}

.check-group .each-item .mian-content .content .toggle-box .remove-btn {
  flex: 0 0 30%;
  width: 30%;
  background-color: #dc0707;
  text-align: center;
  line-height: 95px;
}

.check-group .each-item .mian-content .content .toggle-box .remove-btn .title {
  font-size: 16px;
  color: #fff;
}

.check-group .each-item .mian-content .content .left-area {
  flex: 1;
  padding: 0 0 0 10px;
}

.check-group .each-item .mian-content .content .left-area .title {
  display: block;
  font-weight: 200;
  font-size: 10px;
}

.check-group .each-item .mian-content .content .left-area .desc {
  display: block;
  margin-top: 4px;
  font-weight: 200;
  font-size: 10px;
}

.check-group .each-item .mian-content .content .right-area {
  flex: 0 0 30%;
  width: 30%;
  text-align: center;
  box-sizing: border-box;
}

.check-group .each-item .mian-content .content .right-area .sale-price {
  display: block;
  color: #dc0707;
  font-size: 14px;
}

.check-group .each-item .mian-content .content .right-area .number {
  display: block;
  margin-top: 16px;
}

