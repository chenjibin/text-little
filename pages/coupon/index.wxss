page {
  background-color: #f8f8f8;
}

.fs-get-coupon .fs-coupon-block:nth-of-type(odd) {
    padding: 0 2px 0 4px;

}
.fs-get-coupon .fs-coupon-block:nth-of-type(even) {
    padding: 0 4px 0 2px;

}
.fs-coupon-block {
  display: inline-block;
  width: 50%;
  box-sizing: border-box;
  margin-top: 6px;
}

.fs-coupon-box {
  width: 100%;
  height: 80px;
  box-sizing: border-box;
  position: relative;
}

.fs-coupon-box.gg .top {
}

.fs-coupon-box.gg .top .desc {
  color: #888;
}

.fs-coupon-box.gg .top .num {
  color: #888;
}

.fs-coupon-box.gg .top .num::before {
  color: #888;
}

.fs-coupon-box.gg .bottom {
  background: -webkit-linear-gradient(#cacaca, #bfbfbf);
  background: linear-gradient(#cacaca, #bfbfbf);
}

.fs-coupon-box .top {
  display: flex;
  overflow: hidden;
  padding-top: 12px;
  padding-left: 6px;
  width: 100%;
  height: 70%;
  background-color: #e5e1f2;
  border-radius: 4px 4px 0 0;
  box-sizing: border-box;
  
}

.fs-coupon-box .top::before {
  content: '';
  display: block;
  width: 8px;
  height: 8px;
  position: absolute;
  top: -4px;
  right: 38px;
  z-index: 11;
  background: #f8f8f8;
  border-radius: 50%;
}
.fs-coupon-box .top .desc {
  flex: 1;
  padding-top: 6px;
  padding-left: 4px;
  font-size: 12px;
}

.fs-coupon-box .top .symbol {
  color: #4312c0;
}
.fs-coupon-box .top .num {
  font-size: 20px;
  font-weight: 700;
  color: #4312c0;
}

.fs-coupon-box .bottom {
  overflow: hidden;
  padding-left: 6px;
  width: 100%;
  height: 30%;
  background: -webkit-linear-gradient(#3f15c2, #4e02a5);
  background: linear-gradient(#3f15c2, #4e02a5);
  border-radius: 0 0 4px 4px;
  box-sizing: border-box;
}

.fs-coupon-box .bottom::after {
  content: '';
  display: block;
  width: 8px;
  height: 8px;
  position: absolute;
  right: 38px;
  z-index: 11;
  background: #f8f8f8;
  border-radius: 50%;
}

.fs-coupon-box .bottom::after {
  bottom: -4px;
}

.fs-coupon-box .bottom .title {
  color: #fff;
  font-size: 12px;
}

.fs-coupon-box .line {
  height: 100%;
  border-left: 2px dotted #f8f8f8;
  width: 2px;
  position: absolute;
  z-index: 101;
  right: 39px;
  top: 0;
}

.fs-get-box {
  width: 100%;
  margin: 0 auto;
  padding: 4px 0;
}

.fs-get-box .left {
  color: #888;
  display: inline-block;
  vertical-align: middle;
  width: 64px;
}

.fs-get-box .progress {
  display: inline-block;
  vertical-align: middle;
  width: calc(100% - 72px);
  height: 10px;
  background-color: #ddd;
  border-radius: 10px;
  background-image: -webkit-linear-gradient(left, #ddd 50%, #fb9a9a 50%);
  background-image: linear-gradient(to right, #ddd 50%, #fb9a9a 50%);
  background-position: -100%;
  background-size: 200%;
  -webkit-transition: background-position 2s ease-in-out;
  transition: background-position 2s ease-in-out;
}

.fs-get-box .over {
  color: #888;
  font-size: 16px;
}
